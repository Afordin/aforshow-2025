---
import CTA from "../components/CTA.astro";
import Footer from "../components/Footer.astro";
import FrequentQuestions from "../components/FrequentQuestions.astro";
import Hero from "../components/Hero.astro";
import Navbar from "../components/Navbar/Navbar.astro";
import ShareInfoSection from "../components/ShareInfoSection.astro";
import Talks from "../components/Talks.astro";
import TicketAtropo from "../components/TicketAtropo.astro";
import Layout from "../layouts/Layout.astro";
---

<Layout>
  <Navbar />
  <Hero />
  <main
    class="w-full max-w-4xl mx-auto text-cs-black min-h-screen gap-[80px] my-32 flex flex-col items-center"
  >
    <ShareInfoSection />
    <Talks />
    <TicketAtropo />
    <FrequentQuestions />
    <CTA />
    <Footer />
  </main>
</Layout>
